output "Dell Inc. DELL P2720DC 5FZKKF3" {
  mode 2560x1440
  position 1440,440
  scale 1
  alias $HOME_1
}

output "BOE NE135A1M-NY1 Unknown" {
  mode 2880x1920
  position 0,920
  scale 2
  alias $INTERNAL
}

profile home {
  output $INTERNAL disable
  output $HOME_1 enable
}

profile lap {
  output $INTERNAL enable
}
